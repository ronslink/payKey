{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "testTimeout": 30000,
  "setupFilesAfterEnv": [
    "<rootDir>/env-setup.ts",
    "<rootDir>/test-setup.ts"
  ],
  "maxWorkers": 1,
  "verbose": true,
  "bail": false,
  "forceExit": true,
  "detectOpenHandles": true
}