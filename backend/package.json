    "@faker-js/faker": "^10.2.0",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.11",
    "@types/bcrypt": "^6.0.0",
    "@types/dockerode": "^4.0.1",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.14",
