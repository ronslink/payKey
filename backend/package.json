{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "engines": {
    "npm": ">=11.0.0"
  },
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "seed": "ts-node data_import/import-sam-olago.ts",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "test:unit": "jest --testPathPatterns=src/.*\\.spec\\.ts$ --coverage",
    "test:integration": "jest --testPathPatterns=test/integration/ --testPathPatterns=test/e2e/ --coverage",
    "test:security": "jest --testPathPatterns=test/security/ --coverage",
    "test:compliance": "jest --testPathPatterns=test/compliance/ --coverage",
    "test:performance": "jest --testPathPattern=test/performance/ --timeout=30000",
    "test:coverage-report": "npm run test:cov && node scripts/check-coverage.js",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "typeorm:migration:generate": "typeorm-ts-node-commonjs migration:generate -d ormconfig.ts",
    "typeorm:migration:run": "typeorm-ts-node-commonjs migration:run -d ormconfig.ts",
    "typeorm:migration:revert": "typeorm-ts-node-commonjs migration:revert -d ormconfig.ts",
    "typeorm:schema:sync": "typeorm-ts-node-commonjs schema:sync -d ormconfig.ts"
  },
